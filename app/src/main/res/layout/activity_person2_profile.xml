<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".person2_profile">

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="236dp"
        android:layout_marginEnd="25dp"
        android:text="Slide -> for more photos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/filter_button2" />

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPager2"
        android:layout_width="420dp"
        android:layout_height="380dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/name_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/filter_button2" />

    <Button
        android:id="@+id/settingsbutton3"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#a29ecd"
        android:text="Setting"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/profilebutton" />

    <Button
        android:id="@+id/profilebutton3"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#a29ecd"
        android:text="My Profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/settingsbutton3"
        app:layout_constraintStart_toEndOf="@+id/chatbutton3" />

    <Button
        android:id="@+id/chatbutton3"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#a29ecd"
        android:text="Chat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/profilebutton3"
        app:layout_constraintStart_toEndOf="@+id/matchbutton3" />

    <Button
        android:id="@+id/matchbutton3"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#a29ecd"
        android:text="Match"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/chatbutton3"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/backbtn2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="412dp"
        android:backgroundTint="#acb9e8"
        android:text="Back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_textview2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginTop="464dp"
        android:text="George,"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/filter_button2"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginStart="244dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="17dp"
        android:text="filter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/intestest4_text2"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:layout_marginStart="216dp"
        android:layout_marginTop="584dp"
        android:text="Talking"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/interest1_text2"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="528dp"
        android:text="Gardening"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/interest3_text2"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:layout_marginStart="216dp"
        android:layout_marginTop="528dp"
        android:text="Biking"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/interest2_text2"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="584dp"
        android:text="Cooking"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/skipbtn2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="412dp"
        android:layout_marginEnd="32dp"
        android:backgroundTint="#acb9e8"
        android:text="SKIP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/age_textview3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="228dp"
        android:layout_marginTop="464dp"
        android:text="68"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/likebtn2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="412dp"
        android:layout_marginEnd="146dp"
        android:backgroundTint="#acb9e8"
        android:text="Like"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/location_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="488dp"
        android:text="Pentincton, Kelowna BC"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>